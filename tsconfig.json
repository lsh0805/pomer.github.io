{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.astro",
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@lib/*": [
        "lib/*"
      ]
    }
  }
}